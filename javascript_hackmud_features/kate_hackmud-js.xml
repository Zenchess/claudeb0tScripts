    <contexts>
      <context name="Normal" attribute="Normal Text" lineEndContext="#stay">
        <!-- Comments -->
        <Detect2Chars char="/" char1="/" attribute="Comment" context="SingleLineComment"/>
        <Detect2Chars char="/" char1="*" attribute="Comment" context="MultiLineComment" beginRegion="Comment"/>
        
        <!-- Strings -->
        <DetectChar char="&quot;" attribute="String" context="DoubleQuoteString"/>
        <DetectChar char="'" attribute="String" context="SingleQuoteString"/>
        <DetectChar char="`" attribute="Template String" context="TemplateString"/>
        
        <!-- Regex -->
        <RegExpr String="/[^/\n]+/[gimsuvy]*" attribute="Regular Expression" context="#stay"/>
        
        <!-- Numbers -->
        <RegExpr String="0[xX][0-9a-fA-F]+n?" attribute="Number" context="#stay"/>
        <RegExpr String="0[oO][0-7]+n?" attribute="Number" context="#stay"/>
        <RegExpr String="0[bB][01]+n?" attribute="Number" context="#stay"/>
        <RegExpr String="\d+\.?\d*([eE][-+]?\d+)?n?" attribute="Number" context="#stay"/>
        <RegExpr String="\.\d+([eE][-+]?\d+)?" attribute="Number" context="#stay"/>
        
        <!-- Keywords -->
        <keyword String="keywords" attribute="Keyword" context="#stay"/>
        <keyword String="unsupported_keywords" attribute="Unsupported" context="#stay"/>
        
        <!-- Built-in Objects -->
        <keyword String="builtin_objects" attribute="Built-in Object" context="#stay"/>
        <keyword String="unsupported_objects" attribute="Unsupported" context="#stay"/>
        
        <!-- Hackmud Globals -->
        <keyword String="hackmud_globals" attribute="Hackmud Global" context="#stay"/>
        
        <!-- Hackmud Function Parameters - special italic highlighting -->
        <!-- These are the common names for function(context, args) parameters -->
        <keyword String="hackmud_context_param" attribute="Context Parameter" context="#stay"/>
        <keyword String="hackmud_args_param" attribute="Args Parameter" context="#stay"/>
        
        <!-- Hackmud Color Codes (#0-#9, #a-#z) - used in Xstring backtick syntax -->
        <RegExpr String="#[0-9a-zA-Z]\b" attribute="Hackmud Color Code" context="#stay"/>
        
        <!-- ============================================ -->
        <!-- TRUST SCRIPTS - Split coloring: prefix.username.script -->
        <!-- Trust user = orange (#FF8000), script = green (#1EFF00) -->
        <!-- ============================================ -->
        
        <!-- Trust script calls: #fs.TRUST_USER.script - orange user, green script -->
        <!-- FULLSEC trust scripts (sys, accts, etc.) -->
        <RegExpr String="#fs\." attribute="Scriptor Fullsec" context="TrustScriptorUser"/>
        <!-- HIGHSEC trust scripts -->
        <RegExpr String="#hs\." attribute="Scriptor Highsec" context="TrustScriptorUser"/>
        <!-- MIDSEC trust scripts -->
        <RegExpr String="#ms\." attribute="Scriptor Midsec" context="TrustScriptorUser"/>
        <!-- LOWSEC trust scripts -->
        <RegExpr String="#ls\." attribute="Scriptor Lowsec" context="TrustScriptorUser"/>
        <!-- NULLSEC trust scripts -->
        <RegExpr String="#ns\." attribute="Scriptor Nullsec" context="TrustScriptorUser"/>
        <!-- Generic #s scriptor -->
        <RegExpr String="#s\." attribute="Scriptor Generic" context="UserScriptorUser"/>
        
        <!-- Database access: #db.method() -->
        <RegExpr String="#db\." attribute="Hackmud DB" context="DbMethod"/>
        
        <!-- Macros: #D(), #G, #FMCL, #4S -->
        <RegExpr String="#D\b" attribute="Hackmud Macro" context="#stay"/>
        <RegExpr String="#G\b" attribute="Hackmud Macro" context="#stay"/>
        <RegExpr String="#FMCL\b" attribute="Hackmud Macro" context="#stay"/>
        <RegExpr String="#4S\b" attribute="Hackmud Macro" context="#stay"/>
        
        <!-- Object/Parameter Key - identifier followed by colon (key: value) -->
        <RegExpr String="[a-zA-Z_$][a-zA-Z0-9_$]*(?=\s*:)" attribute="Parameter Key" context="#stay"/>
        
        <!-- Arrow Function -->
        <RegExpr String="=&gt;" attribute="Arrow" context="#stay"/>
        
        <!-- Unsupported Operators (parse error) -->
        <Detect2Chars char="?" char1="." attribute="Unsupported" context="#stay"/>
        <Detect2Chars char="?" char1="?" attribute="Unsupported" context="#stay"/>
        <Detect2Chars char="*" char1="*" attribute="Unsupported" context="#stay"/>
        
        <!-- Supported Methods -->
        <keyword String="array_methods" attribute="Method" context="#stay"/>
        <keyword String="object_methods" attribute="Method" context="#stay"/>
        <keyword String="string_methods" attribute="Method" context="#stay"/>
        <keyword String="math_members" attribute="Method" context="#stay"/>
        <keyword String="scripts_lib_functions" attribute="Method" context="#stay"/>
        <keyword String="unsupported_methods" attribute="Unsupported" context="#stay"/>
        
        <!-- Identifiers -->
        <RegExpr String="[a-zA-Z_$][a-zA-Z0-9_$]*" attribute="Normal Text" context="#stay"/>
        
        <!-- Brackets - structural, high visibility -->
        <AnyChar String="{}[]()" attribute="Bracket" context="#stay"/>
        
        <!-- Punctuation - subtle separators -->
        <AnyChar String=".,;:" attribute="Punctuation" context="#stay"/>
        
        <!-- Operators - math/logic/comparison -->
        <AnyChar String="+-*/%&amp;|^~!&lt;&gt;=?" attribute="Operator" context="#stay"/>
      </context>
      
      <!-- ============================================ -->
      <!-- Context: Trust Scriptor Username (orange) -->
      <!-- Matches trust usernames: sys, accts, chats, etc. -->
      <!-- ============================================ -->
      <context name="TrustScriptorUser" attribute="Trust Username" lineEndContext="#pop">
        <!-- Known trust script prefixes get orange -->
        <keyword String="trust_script_prefixes" attribute="Trust Username" context="TrustScriptorDot"/>
        <!-- Fallback: any identifier as trust username -->
        <RegExpr String="[a-z_][a-z0-9_]*" attribute="Trust Username" context="TrustScriptorDot"/>
      </context>
      
      <!-- Dot after trust username -->
      <context name="TrustScriptorDot" attribute="Punctuation" lineEndContext="#pop#pop">
        <DetectChar char="." attribute="Punctuation" context="TrustScriptorScript"/>
        <!-- No dot found, pop back -->
        <RegExpr String="." attribute="Normal Text" context="#pop#pop" lookAhead="true"/>
      </context>
      
      <!-- Script name after trust username (green) -->
      <context name="TrustScriptorScript" attribute="Script Name" lineEndContext="#pop#pop#pop">
        <!-- Known trust script names get green -->
        <keyword String="trust_script_names" attribute="Script Name" context="#pop#pop#pop"/>
        <!-- Any script name gets green -->
        <RegExpr String="[a-z_][a-z0-9_]*" attribute="Script Name" context="#pop#pop#pop"/>
      </context>
      
      <!-- ============================================ -->
      <!-- Context: User Scriptor Username (gray) -->
      <!-- For #s.username.script - regular user scripts -->
      <!-- ============================================ -->
      <context name="UserScriptorUser" attribute="User Username" lineEndContext="#pop">
        <RegExpr String="[a-z_][a-z0-9_]*" attribute="User Username" context="UserScriptorDot"/>
      </context>
      
      <!-- Dot after user username -->
      <context name="UserScriptorDot" attribute="Punctuation" lineEndContext="#pop#pop">
        <DetectChar char="." attribute="Punctuation" context="UserScriptorScript"/>
        <RegExpr String="." attribute="Normal Text" context="#pop#pop" lookAhead="true"/>
      </context>
      
      <!-- Script name after user username (green) -->
      <context name="UserScriptorScript" attribute="Script Name" lineEndContext="#pop#pop#pop">
        <RegExpr String="[a-z_][a-z0-9_]*" attribute="Script Name" context="#pop#pop#pop"/>
      </context>
      
      <!-- ============================================ -->
      <!-- Context: Database method -->
      <!-- #db.f(), #db.i(), etc. -->
      <!-- ============================================ -->
      <context name="DbMethod" attribute="Hackmud DB" lineEndContext="#pop">
        <keyword String="db_methods" attribute="Hackmud DB" context="#pop"/>
        <RegExpr String="[a-zA-Z_][a-zA-Z0-9_]*" attribute="Hackmud DB" context="#pop"/>
      </context>
      
      <!-- Comments -->
      <context name="SingleLineComment" attribute="Comment" lineEndContext="#pop">
        <IncludeRules context="##Comments"/>
      </context>
      
      <context name="MultiLineComment" attribute="Comment" lineEndContext="#stay">
        <Detect2Chars char="*" char1="/" attribute="Comment" context="#pop" endRegion="Comment"/>
        <IncludeRules context="##Comments"/>
      </context>
      
      <!-- Strings -->
      <context name="DoubleQuoteString" attribute="String" lineEndContext="#pop">
        <DetectChar char="&quot;" attribute="String" context="#pop"/>
        <RegExpr String="\\." attribute="String Escape" context="#stay"/>
      </context>
      
      <context name="SingleQuoteString" attribute="String" lineEndContext="#pop">
        <DetectChar char="'" attribute="String" context="#pop"/>
        <RegExpr String="\\." attribute="String Escape" context="#stay"/>
      </context>
      
      <context name="TemplateString" attribute="Template String" lineEndContext="#stay">
        <DetectChar char="`" attribute="Template String" context="#pop"/>
        <Detect2Chars char="$" char1="{" attribute="Template Interpolation" context="TemplateInterpolation"/>
        <RegExpr String="\\." attribute="String Escape" context="#stay"/>
      </context>
      
      <context name="TemplateInterpolation" attribute="Normal Text" lineEndContext="#stay">
        <DetectChar char="}" attribute="Template Interpolation" context="#pop"/>
        <IncludeRules context="Normal"/>
      </context>
    </contexts>
    
    <itemDatas>
      <!-- VS Code Dark+ Theme Colors -->
      <itemData name="Normal Text"           defStyleNum="dsNormal"     spellChecking="false" color="#D4D4D4"/>
      <itemData name="Keyword"               defStyleNum="dsKeyword"    spellChecking="false" color="#569CD6" bold="false"/>
      <itemData name="Built-in Object"       defStyleNum="dsDataType"   spellChecking="false" color="#4EC9B0"/>
      <itemData name="Method"                defStyleNum="dsFunction"   spellChecking="false" color="#DCDCAA"/>
      <itemData name="String"                defStyleNum="dsString"     spellChecking="false" color="#CE9178"/>
      <itemData name="Template String"       defStyleNum="dsString"     spellChecking="false" color="#CE9178"/>
      <itemData name="Template Interpolation" defStyleNum="dsSpecialChar" spellChecking="false" color="#569CD6"/>
      <itemData name="String Escape"         defStyleNum="dsSpecialChar" spellChecking="false" color="#D7BA7D"/>
      <itemData name="Number"                defStyleNum="dsDecVal"     spellChecking="false" color="#B5CEA8"/>
      <itemData name="Regular Expression"    defStyleNum="dsSpecialString" spellChecking="false" color="#D16969"/>
      <itemData name="Comment"               defStyleNum="dsComment"    spellChecking="true"  color="#6A9955" italic="true"/>
      <itemData name="Bracket"               defStyleNum="dsOperator"   spellChecking="false" color="#DA70D6"/>
      <itemData name="Punctuation"          defStyleNum="dsOperator"   spellChecking="false" color="#808080"/>
      <itemData name="Operator"             defStyleNum="dsOperator"   spellChecking="false" color="#B4B4B4"/>
      <itemData name="Arrow"                 defStyleNum="dsOperator"   spellChecking="false" color="#569CD6"/>
      <itemData name="Hackmud Global"        defStyleNum="dsVariable"   spellChecking="false" color="#9CDCFE"/>
      
      <!-- Function Parameter Special Highlighting (italic + unique colors) -->
      <!-- Context parameter = Light Blue #9CDCFE, italic -->
      <itemData name="Context Parameter"    defStyleNum="dsVariable"   spellChecking="false" color="#9CDCFE" italic="true" bold="false"/>
      <!-- Args parameter = Yellow #DCDCAA, italic -->
      <itemData name="Args Parameter"       defStyleNum="dsVariable"   spellChecking="false" color="#DCDCAA" italic="true" bold="false"/>
      
      <!-- Trust-Level Scriptor Prefixes (the #fs, #hs, etc. part) -->
      <itemData name="Scriptor Fullsec"     defStyleNum="dsPreprocessor" spellChecking="false" color="#1EFF00" bold="true"/>
      <itemData name="Scriptor Highsec"     defStyleNum="dsPreprocessor" spellChecking="false" color="#4EC9B0" bold="true"/>
      <itemData name="Scriptor Midsec"      defStyleNum="dsPreprocessor" spellChecking="false" color="#DCDCAA" bold="true"/>
      <itemData name="Scriptor Lowsec"      defStyleNum="dsPreprocessor" spellChecking="false" color="#FF8000" bold="true"/>
      <itemData name="Scriptor Nullsec"     defStyleNum="dsPreprocessor" spellChecking="false" color="#F44747" bold="true"/>
      <itemData name="Scriptor Generic"     defStyleNum="dsPreprocessor" spellChecking="false" color="#808080"/>
      
      <!-- Scriptor Parts - Username and Script Name (hackmud wiki colors) -->
      <!-- Trust username (sys, accts, etc.) = Orange #FF8000 (color F in hackmud) -->
      <itemData name="Trust Username"       defStyleNum="dsPreprocessor" spellChecking="false" color="#FF8000" bold="true"/>
      <!-- User username (regular users) = Gray #9B9B9B (color C in hackmud) -->
      <itemData name="User Username"        defStyleNum="dsPreprocessor" spellChecking="false" color="#9B9B9B"/>
      <!-- Script name = Green #1EFF00 (color L in hackmud) -->
      <itemData name="Script Name"          defStyleNum="dsPreprocessor" spellChecking="false" color="#1EFF00" bold="true"/>
      
      <!-- Database and Macros -->
      <itemData name="Hackmud DB"            defStyleNum="dsPreprocessor" spellChecking="false" color="#C586C0"/>
      <itemData name="Hackmud Macro"         defStyleNum="dsPreprocessor" spellChecking="false" color="#C586C0" bold="true"/>
      
      <!-- Hackmud Color Codes (#0-#9) - same as hackmud in-game color syntax -->
      <itemData name="Hackmud Color Code"   defStyleNum="dsPreprocessor" spellChecking="false" color="#9B9B9B"/>
      
      <!-- Parameter Highlighting (hackmud output style) -->
      <!-- Parameter key = Cyan #00FFFF (color N in hackmud) -->
      <itemData name="Parameter Key"        defStyleNum="dsAttribute"  spellChecking="false" color="#00FFFF"/>
      
      <!-- Errors -->
      <itemData name="Unsupported"           defStyleNum="dsError"      spellChecking="false" color="#F44747" underline="true" bold="true"/>
    </itemDatas>
  </highlighting>
  
  <general>
    <comments>
      <comment name="singleLine" start="//"/>
      <comment name="multiLine" start="/*" end="*/"/>
    </comments>
    <keywords casesensitive="true"/>
  </general>
