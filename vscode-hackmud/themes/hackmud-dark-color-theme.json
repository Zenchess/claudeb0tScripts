{
  "name": "Hackmud Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorLineNumber.foreground": "#858585",
    "editorLineNumber.activeForeground": "#c6c6c6",
    "editor.selectionBackground": "#264f78",
    "editor.inactiveSelectionBackground": "#3a3d41"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "comment.block", "comment.line"],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#D7BA7D"
      }
    },
    {
      "name": "Template Expression",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Language Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Variable Language",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Builtin Object",
      "scope": ["support.class.builtin"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Arrow Function",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Hackmud Global",
      "scope": ["variable.other.hackmud-global"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Hackmud Scriptor",
      "scope": ["entity.name.function.scriptor", "entity.name.function.db", "entity.name.function.macro"],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#D16969"
      }
    },
    {
      "name": "Invalid/Unsupported",
      "scope": ["invalid.illegal"],
      "settings": {
        "foreground": "#F44747",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    }
  ]
}
